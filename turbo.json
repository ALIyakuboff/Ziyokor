{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    },
    "globalEnv": [
        "DATABASE_URL",
        "DATABASE_URL1",
        "JWT_SECRET",
        "DB_AUTO_INIT",
        "TZ",
        "ENABLE_EXPERIMENTAL_COREPACK",
        "CRON_SECRET",
        "tz"
    ],
    "globalPassThroughEnv": [
        "DATABASE_URL",
        "DATABASE_URL1",
        "JWT_SECRET",
        "DB_AUTO_INIT",
        "TZ",
        "ENABLE_EXPERIMENTAL_COREPACK",
        "CRON_SECRET",
        "tz"
    ]
}