{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
    }
},
"globalEnv": [
    "DATABASE_URL",
    "DATABASE_URL1",
    "JWT_SECRET",
    "DB_AUTO_INIT",
    "TZ"
],
"globalPassThroughEnv": [
    "DATABASE_URL",
    "DATABASE_URL1",
    "JWT_SECRET",
    "DB_AUTO_INIT",
    "TZ"
]
}