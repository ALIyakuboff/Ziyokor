{
    "version": 2,
    "buildCommand": "echo 'DEBUG START' && pwd && ls -la && pnpm --filter work-control-portal build && echo 'DEBUG END' && ls -la && echo 'LS DIST:' && ls -la dist",
    "installCommand": "npx pnpm@9.15.4 install --no-frozen-lockfile",
    "outputDirectory": "dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/socket.io/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/((?!assets/|.*\\..*|api/).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/close-day",
            "schedule": "0 0 * * *"
        }
    ]
}