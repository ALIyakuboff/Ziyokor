{
    "version": 2,
    "installCommand": "pnpm install --no-frozen-lockfile",
    "rewrites": [
        {
            "source": "/socket.io/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "handle": "filesystem"
        },
        {
            "source": "/((?!assets/|.*\\..*|api/).*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "apps/web-portal/dist",
    "framework": null
}